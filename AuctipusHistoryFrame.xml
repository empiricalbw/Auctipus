<?xml version="1.0"?>
<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
    <Button name="AuctipusHistoryBarTemplate" virtual="true">
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Top" file="Interface\Addons\Auctipus\Bar.tga">
                    <Size y="5"/>
                    <Anchors>
                        <Anchor point="TOPLEFT"/>
                        <Anchor point="TOPRIGHT"/>
                    </Anchors>
                    <TexCoords left="0" right="1" top="0" bottom="0.25"/>
                </Texture>
                <Texture parentKey="Bottom" file="Interface\Addons\Auctipus\Bar.tga">
                    <Size y="5"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT"/>
                        <Anchor point="BOTTOMRIGHT"/>
                    </Anchors>
                    <TexCoords left="0" right="1" top="0.75" bottom="1"/>
                </Texture>
                <Texture parentKey="Middle" file="Interface\Addons\Auctipus\Bar.tga">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Top" relativePoint="BOTTOMLEFT"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.Bottom" relativePoint="TOPRIGHT"/>
                    </Anchors>
                    <TexCoords left="0" right="1" top="0.25" bottom="0.75"/>
                </Texture>
            </Layer>
        </Layers>
    </Button>

    <Frame name="AuctipusHistoryFrameTemplate" enableMouse="true" virtual="true" mixin="AuctipusHistoryFrame">
        <Size x="832" y="440"/>
        <Frames>
            <Frame name="$parentSearchBoxLabel" parentKey="SearchBoxLabel">
                <Size x="50" y="24"/>
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-7"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <FontString name="$parentFont" inherits="GameFontNormalSmall" justifyH="RIGHT" justifyV="MIDDLE" text="Search:"/>
                    </Layer>
                </Layers>
            </Frame>
            <EditBox name="$parentSearchBox" parentKey="SearchBox" parentArray="EditBoxes" enableMouse="true" autoFocus="false" letters="63"
             inherits="BackdropTemplate">
                <KeyValues>
                    <KeyValue key="backdropInfo" value="BACKDROP_TOOLTIP_16_16_5555" type="global"/>
                    <KeyValue key="backdropColor" value="TOOLTIP_DEFAULT_BACKGROUND_COLOR" type="global"/>
                </KeyValues>
                <Size x="150" y="24"/>
                <Anchors>
                    <Anchor point="LEFT" relativeTo="$parentSearchBoxLabel" relativePoint="RIGHT"/>
                </Anchors>
                <TextInsets left="5" right="0" top="0" bottom="0"/>
                <FontString inherits="ChatFontNormal"/>
            </EditBox>

            <Button name="$parentFullScanButton" parentKey="FullScanButton" inherits="UIPanelButtonTemplate" text="Full Scan">
                <Size x="100" y="23"/>
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent.SearchBox" relativePoint="RIGHT" x="10"/>
                </Anchors>
            </Button>

            <Button name="$parentHistoryGroup1" parentKey="HistoryGroup1" parentArray="HistoryGroupRows" inherits="AuctipusHistoryGroupRowTemplate"/>
            <Button name="$parentHistoryGroup2" parentArray="HistoryGroupRows" inherits="AuctipusHistoryGroupRowTemplate"/>
            <Button name="$parentHistoryGroup3" parentArray="HistoryGroupRows" inherits="AuctipusHistoryGroupRowTemplate"/>
